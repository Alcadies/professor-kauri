  "devDependencies": {
    "@types/common-tags": "^1.8.0",
    "@types/dotenv": "^6.1.1",
    "@types/mongoose": "^5.5.12",
    "@types/node": "^12.7.2",
    "@types/node-emoji": "^1.8.1",
    "@types/node-fetch": "^2.5.0",
    "@types/string-similarity": "^3.0.0",
    "@types/ws": "^6.0.2",
